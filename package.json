{
  "name": "stelace",
  "version": "0.0.8",
  "description": "Client for Stelace API",
  "main": "dist/stelace.node.js",
  "module": "dist/es-modules/stelace.js",
  "jsnext:main": "dist/es-modules/stelace.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:modules && npm run build:standalone",
    "build:modules": "BABEL_ENV=modules babel lib -d dist/es-modules/",
    "build:standalone": "NODE_ENV=development webpack && NODE_ENV=production webpack",
    "lint": "eslint .",
    "test": "BABEL_ENV=test node ./test/runner | tap-spec",
    "test:ci": "BABEL_ENV=test node ./test/runner | tape-xunit",
    "test:browser-local": "BABEL_ENV=test karma start karma.conf.local.js",
    "test:prepush": "npm run build && npm run test",
    "precommit": "npm run lint"
  },
  "author": "Stelace Team",
  "license": "MIT",
  "homepage": "https://stelace.com",
  "keywords": [
    "stelace",
    "api",
    "marketplace"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stelace/stelace-js-sdk"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "jwt-decode": "^2.2.0",
    "localstorage-memory": "^1.0.2",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.22.1",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^7.1.3",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "blue-tape": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsonwebtoken": "^8.3.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-tap": "^4.1.4",
    "karma-webpack": "^3.0.5",
    "lodash-webpack-plugin": "^0.11.5",
    "moxios": "^0.4.0",
    "require-all": "^3.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.3.4",
    "tap-spec": "^5.0.0",
    "tap-xunit": "^2.3.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "engines": {
    "node": ">=6"
  }
}
